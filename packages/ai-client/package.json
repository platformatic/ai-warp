{
  "name": "@platformatic/ai-client",
  "version": "0.6.0-dev.1",
  "description": "The Platformatic AI client",
  "license": "Apache-2.0",
  "scripts": {
    "test": "node --test --experimental-strip-types test/*.test.ts",
    "test:coverage": "node --test --experimental-strip-types --experimental-test-coverage  --test-coverage-exclude=test/** test/*.test.ts",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist;",
    "check": "pnpm clean && pnpm lint && pnpm typecheck && pnpm test && pnpm build"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "dependencies": {
    "split2": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/split2": "^4.2.3",
    "c8": "^10.1.3",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "^9.21.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "neostandard": "^0.12.1",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ]
}
