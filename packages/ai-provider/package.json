{
  "name": "@platformatic/ai-provider",
  "version": "1.1.0-alpha.3",
  "description": "The Platformatic AI provider interface",
  "homepage": "https://github.com/platformatic/ai-warp",
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/ai-warp.git"
  },
  "keywords": [
    "ai",
    "provider"
  ],
  "bugs": {
    "url": "https://github.com/platformatic/ai-warp/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --cache",
    "test": "c8 --reporter text --reporter html node --disable-warning=ExperimentalWarning --experimental-strip-types --test --test-reporter=cleaner-spec-reporter --test-concurrency=1 --test-timeout=300000 test/*.test.ts",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm run lint && pnpm run test && pnpm run typecheck && pnpm run build",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@fastify/error": "^4.2.0",
    "cloneable-readable": "^3.0.0",
    "fast-json-stringify": "^6.0.1",
    "iovalkey": "^0.3.3",
    "openai": "^5.7.0",
    "undici": "^7.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "c8": "^10.1.3",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "^9.21.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "neostandard": "^0.12.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
