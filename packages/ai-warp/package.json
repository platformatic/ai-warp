{
  "name": "@platformatic/ai-warp",
  "version": "1.0.0",
  "description": "The Platformatic AI provider interface",
  "homepage": "https://github.com/platformatic/ai-warp",
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/ai-warp.git"
  },
  "keywords": [
    "ai",
    "warp"
  ],
  "bugs": {
    "url": "https://github.com/platformatic/ai-warp/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "node -e 'import {schema} from \"./src/lib/schema.ts\"; console.log(JSON.stringify(schema, null, 2))' > schema.json && json2ts > src/config.d.ts < schema.json",
    "postbuild": "cp src/config.d.ts dist/config.d.ts",
    "lint": "eslint --cache",
    "test": "c8 --reporter text --reporter html node --disable-warning=ExperimentalWarning --experimental-strip-types --test --test-reporter=cleaner-spec-reporter --test-concurrency=1 --test-timeout=300000 test/*.test.ts",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm run lint && pnpm run test && pnpm run typecheck && pnpm run build",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "@fastify/error": "^4.2.0",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@platformatic/fastify-ai": "workspace:*",
    "@platformatic/foundation": "^3.0.2",
    "@platformatic/service": "^3.0.2",
    "ajv": "^8.12.0",
    "fastify-plugin": "^5.0.1",
    "fastify-user": "^1.4.0"
  },
  "devDependencies": {
    "@platformatic/generators": "^3.0.2",
    "@types/node": "^22.13.9",
    "c8": "^10.1.3",
    "cleaner-spec-reporter": "^0.5.0",
    "eslint": "^9.21.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-jwt": "^4.0.0",
    "fastify": "^5.2.1",
    "json-schema-to-typescript": "^15.0.4",
    "neostandard": "^0.12.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
